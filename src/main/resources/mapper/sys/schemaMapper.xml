<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gydz.user.mapper.schemaMapper">
	
	<select id="getTables"
		statementType="STATEMENT" resultType="String">
		select table_name from information_schema.tables where table_schema='ipmi';
	</select>
	
</mapper>